<!DOCTYPE html><html><head>    <meta charset="utf-8" />    <title>@ViewBag.Title</title>    <environment include="Development">        <link href="~/node_modules/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />        <link href="~/node_modules/font-awesome/css/font-awesome.min.css" rel="stylesheet" />        <link href="~/css/site.css" rel="stylesheet" />        <meta charset="UTF-8">    </environment>    <environment exclude="Development">        <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet" />        <link href="~/dist/webapp.min.css" rel="stylesheet" asp-append-version="true" />    </environment>    @RenderSection("head", false)</head><body>    @*<section>        <nav>            <h4>developedbyed</h4>            <a class="button">Sign Up</a>        </nav>    </section>*@    @*<section>*@        <header class="font-text">            <nav class="container-xxl flex-wrap navbar navbar-dark navbar-expand-md bd-navbar">                <h1 class="navbar-brand title"><i class="fa fa-mug-hot"></i>DAILY TECHOFFEE</h1>                <button class="navbar-toggler" data-toggle="collapse" data-target="#theMenu">                    <span class="navbar-toggler-icon"></span>                </button>                <div id="theMenu" class="navbar-collapse collapse">                    <ul class="navbar-nav ml-auto">                        <li class="nav-item">                            <a class="nav-link" asp-controller="App" asp-action="Index">Home</a>                        </li>                        <li class="nav-item">                            <a class="nav-link" asp-controller="App" asp-action="Shop">Shop</a>                        </li>                        <li class="nav-item">                            <a class="nav-link" asp-controller="App" asp-action="Shop">Blogs</a>                        </li>                        <li class="nav-item">                            <a class="nav-link" asp-controller="App" asp-action="Contact">Contact</a>                        </li>                        <li class="nav-item dropdown">                            <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">                                Account                            </a>                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">                                @if (User.Identity.IsAuthenticated)                                {                    <a class="dropdown-item" asp-controller="Account" asp-action="Logout">Logout</a> }                else                {    <a class="dropdown-item" asp-controller="Account" asp-action="Login">Login</a>                    <a class="dropdown-item" asp-controller="Account" asp-action="Signup">Signup</a>}                            </div>                        </li>                        <li class="nav-item">                            <a class="nav-link" asp-controller="App" asp-action="About">About</a>                        </li>                    </ul>                </div>            </nav>        </header>        @*  </section> *@        @*<section class="container">            <h2 class="text-center">@ViewBag.Title</h2>            @RenderBody()        </section>*@        <div class="container-xxl my-md-4 bd-layout">            <h2 class="text-center">@ViewBag.Title</h2>            @RenderBody()        </div>        <footer class="container">            <div class="text-center fixed-bottom">                Copyright 2017 Daily Techoffee            </div>        </footer>        <environment include="Development">            <script src="~/node_modules/jquery/dist/jquery.js"></script>            <script src="~/node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>            <script src="~/javascript/index.js"></script>        </environment>        <environment exclude="Development">            <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous" asp-fallback-src="~/node_modules/jquery/dist/jquery.min.js" asp-fallback-test="window.jQuery"></script>            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.bundle.min.js" asp-fallback-src="~/node_modules/bootstrap/dist/js/bootstrap.bundle.min.js" asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal" asp-append-version="true"></script>            <script src="~/dist/webapp.min.js"></script>        </environment>        @RenderSection("scripts", false)        @*false means it is not gonna load renderSection on every page*@</body></html>